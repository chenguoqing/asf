<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <import resource="applicationContext-common.xml"/>

    <bean id="testDefinition" class="com.baidu.asf.model.xml.XMLDefinition">
        <constructor-arg value="/model/asf2.xml"/>
    </bean>

    <bean class="com.baidu.asf.engine.spring.ASFEngineFactoryBean">
        <property name="definition" ref="testDefinition"/>
        <property name="engineConfiguration" ref="engineConfig"/>
    </bean>
</beans>